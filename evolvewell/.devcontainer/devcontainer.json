{
  "name": "EvolveWell",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:22",
  "postCreateCommand": "cd evolvewell && npm install",
  "postAttachCommand": "cd evolvewell && npm run dev",
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "EvolveWell App",
      "onAutoForward": "openBrowser"
    }
  }
}
